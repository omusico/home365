jQuery(function(){
  $('.fagtext').hide();
  jQuery('.btshowhide').click(function(e){
    e.preventDefault();
    var target = jQuery('.class' + jQuery(this).attr('rel'));
    jQuery('.fagtext').not(target).hide();
    target.show();
    jQuery('.btshowhide').not(this).removeClass("active");
    jQuery(this).addClass("active");
  })
});



